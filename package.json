{
  "name": "@chmap/geo-referencing",
  "version": "1.0.2",
  "description": "Users can import a iiif manifest file, then assign a geo-coordinate to image nodes by drag & drop on the leaflet map. Additionally, users can rotate, scale, and distort the shape of an image node for completing a simple geo-referencing.",
  "lib-main": "lib/js/index.js",
  "main": "dis/js/index.js",
  "module": "src/js/index.js",
  "scripts": {
    "build:js": "npm run build:js:clean && webpack --mode=production",
    "build:js:clean": "rm -rf dist/js && mkdir -p dist/js",
    "build:scss": "npm run build:scss:clean && webpack --config webpack-scss.config.js --mode=production",
    "build:scss:clean": "rm -rf dist/css && mkdir -p dist/css",
    "lib": "npm run clean:lib && babel src --out-dir lib --copy-files",
    "clean:lib": "rm -rf lib && mkdir lib"
  },
  "keywords": [
    "gis",
    "map",
    "leaflet"
  ],
  "author": "calvin yeh",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.16.7",
    "@babel/runtime-corejs3": "^7.16.8",
    "@chmap/leaflet-extensions": "^1.0.2",
    "@chmap/utilities": "^1.0.1",
    "bootstrap": "^5.1.3",
    "bootstrap-icons": "^1.8.0",
    "file-saver": "^2.0.5",
    "leaflet": "^1.7.1",
    "leaflet-distortableimage": "^0.21.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.10",
    "@babel/plugin-transform-runtime": "^7.16.10",
    "@babel/preset-env": "^7.16.11",
    "@popperjs/core": "^2.11.2",
    "autoprefixer": "^10.4.2",
    "babel-loader": "^8.2.3",
    "css-loader": "^6.5.1",
    "dotenv-webpack": "^7.1.0",
    "imports-loader": "^3.1.1",
    "mini-css-extract-plugin": "^2.5.3",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.3.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "webpack": "^5.68.0",
    "webpack-cli": "^4.9.2",
    "webpack-fix-style-only-entries": "^0.6.1"
  }
}
